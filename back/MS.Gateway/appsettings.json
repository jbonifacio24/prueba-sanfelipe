{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      },
      "compras-route": {
        "ClusterId": "compras-cluster",
        "Match": {
          "Path": "/compra/{**catch-all}"
        }
      },
      "productos-route": {
        "ClusterId": "productos-cluster",
        "Match": {
          "Path": "/producto/{**catch-all}"
        }
      },
      "ventas-route": {
        "ClusterId": "ventas-cluster",
        "Match": {
          "Path": "/venta/{**catch-all}"
        }
      },
      "movimientos-route": {
        "ClusterId": "movimientos-cluster",
        "Match": {
          "Path": "/movimiento/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-destination": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "compras-cluster": {
        "Destinations": {
          "compras-destination": {
            "Address": "http://localhost:5002/"
          }
        }
      },
      "productos-cluster": {
  "Destinations": {
          "productos-destination": {
            "Address": "http://localhost:5004/"
          }
        }
      },
      "ventas-cluster": {
        "Destinations": {
          "ventas-destination": {
            "Address": "http://localhost:5005/"
          }
        }
      },
      "movimientos-cluster": {
        "Destinations": {
          "movimientos-destination": {
            "Address": "http://localhost:5003/"
          }
        }
      }
    }
  }
}
